ToggleButton {
  color: white
  background-color: #848484
  border: none
  -drop {
    :after {
      background-color: #656565
    }
  }
  -blocking {
    border: 0px
  }
  -unblocking {
    background-color: rgb(212, 112, 112)
    color: white
    border: 0px
  }
  :hover {
    background-color: #585656
  }
  @svg subscribed {
    content: "<circle cx='6' stroke='#FFF' fill='none' cy='6' r='5' /> <circle cx='6' cy='6' r='3' fill='#FFF'/>"
  }
}
